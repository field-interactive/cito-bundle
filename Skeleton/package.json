{
    "devDependencies": {
        "@babel/preset-env": "^7.6.2",
        "@symfony/webpack-encore": "^0.28.0",
        "autoprefixer": "^9.6.1",
        "compression-webpack-plugin": "^3.0.0",
        "core-js": "^3.2.1",
        "node-sass": "^4.12.0",
        "postcss-loader": "^3.0.0",
        "sass-loader": "^7.0.1",
        "webpack-notifier": "^1.8.0",
        "workbox-webpack-plugin": "^4.3.1"
    },
    "license": "UNLICENSED",
    "private": true,
    "sideEffects": false,
    "browserslist": [
        "last 2 major versions",
        "not ie < 11",
        "> 0.5%",
        "not dead",
        "Firefox ESR"
    ],
    "scripts": {
        "dev": "encore dev --config modern.webpack.config.js",
        "watch": "encore dev --watch --config modern.webpack.config.js",
