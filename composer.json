{
    "name": "field-interactive/cito-bundle",
    "type": "symfony-bundle",
    "description": "Field CitoBundle",
    "keywords": ["cito"],
    "homepage": "https://www.field-interactive.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Tim Komperna√ü",
            "email": "t.kompernass@field-interactive.com"
        },
        {
            "name": "Fabian Schmick",
            "email": "f.schmick@field-interactive.com"
        },
        {
            "name": "Marc Harding",
            "email": "m.harding@field-interactive.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.0",
        "liip/imagine-bundle": "^2.0@dev",
        "sensio/framework-extra-bundle": "^5.0",
        "symfony/asset": "^4.0",
        "symfony/console": "^4.0",
        "symfony/twig-bundle": "^4.0",
        "symfony/yaml": "^4.0",
        "symfony/filesystem": "^4.0",
        "symfony/process": "^4.0",
        "composer/ca-bundle": "^1"
    },
    "require-dev": {
        "symfony/dotenv": "^4.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": { "FieldInteractive\\CitoBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "pages-dir": "pages"
    }
}
