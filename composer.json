{
    "name": "field-interactive/cito-bundle",
    "type": "symfony-bundle",
    "description": "Field CitoBundle",
    "keywords": ["cito"],
    "homepage": "https://www.field-interactive.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Tim Komperna√ü",
            "email": "t.kompernass@field-interactive.com"
        },
        {
            "name": "Fabian Schmick",
            "email": "f.schmick@field-interactive.com"
        },
        {
            "name": "Marc Harding",
            "email": "m.harding@field-interactive.com"
        },
        {
            "name": "Dominik Deimel",
            "email": "d.deimel@field-interactive.com"
        },
        {
            "name": "Luca Kermas",
            "email": "l.kermas@field-interactive.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.2",
        "sensio/framework-extra-bundle": "^5.0",
        "symfony/asset": "^4.2",
        "symfony/console": "^4.2",
        "symfony/twig-bundle": "^4.2",
        "symfony/form": "^4.2",
        "symfony/yaml": "^4.2",
        "symfony/filesystem": "^3.0|^4.2",
        "symfony/process": "^3.0|^4.2",
        "liip/imagine-bundle": "^2.0",
        "composer/ca-bundle": "^1",
        "swiftmailer/swiftmailer": "^6.0",
        "symfony/webpack-encore-bundle": "^1.5"
    },
    "require-dev": {
        "symfony/dotenv": "^4.2"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": { "FieldInteractive\\CitoBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "pages-dir": "pages"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/field-interactive/webpack-encore-bundle"
        }
    ]
}
